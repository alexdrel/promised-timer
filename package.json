{
  "author": "Alex Drel",
  "license": "MIT",
  "name": "promised-timer",
  "version": "0.0.7",
  "description": "Promisified Javascript Timer",
  "main": "dist/promised-timer.js",
  "types": "dist/promised-timer.d.ts",
  "engines": {
    "node": ">4.x.x",
    "npm": ">2.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdrel/promised-timer.git"
  },
  "keywords": [
    "timer",
    "promise"
  ],
  "scripts": {
    "build": "npm run lint && tsc -p src -d",
    "build:watch": "tsc --watch -p src -d",
    "clean": "rimraf build/ dist/",
    "pack": "webpack -p",
    "pack:watch": "webpack --watch",
    "start": "webpack-dev-server -d --port 8006",
    "lint": "tslint src/**/*.ts",
    "test": "karma start --singleRun --browsers Firefox",
    "test:once": "karma start --singleRun",
    "test:watch": "karma start"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  }
}
